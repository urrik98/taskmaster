
<div class="container">

  <div class="row">
    <div class="col-md-6 col-md-offset-3" style="background-color:rgba(255,255,255,.5);border-radius:9px 9px 0px 0px;">
      <% @lists.each do |list| %>
        <h3 class="text-justified"><%= link_to list.date.strftime('%B %d, %Y')+' - '+ number_to_percentage(list.completed * 100, :precision => 2)+' complete', list_path(list.id) %></h3>
      <% end %>
    </div>
  </div>

  <div class="row">
    <div class="col-md-6 col-md-offset-3" style="background-color:rgba(255,255,255,.5);border-radius:0px 0px 9px 9px;">
      <br>
      <%= link_to 'New List', new_list_path, :class => "btn btn-default btn-block" %>
      <br>
    </div>
  </div>

</div>
